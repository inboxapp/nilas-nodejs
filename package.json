{
  "name": "nylas",
  "version": "6.8.0",
  "description": "A NodeJS wrapper for the Nylas REST API for email, contacts, and calendar.",
  "main": "lib/nylas.js",
  "types": "lib/nylas.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext .js,.ts -f visualstudio .",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "npm run lint:fix -- --quiet",
    "lint:prettier": "prettier --write '**/*.{ts,js}'",
    "lint:prettier:check": "prettier --check '**/*.{ts,js}'",
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "nodemon -w src --exec npm run build"
  },
  "keywords": [
    "email",
    "contacts",
    "calendar",
    "nylas"
  ],
  "author": "Nylas, Inc.",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.1",
    "sha256": "^0.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@types/node-fetch": "^3.0.3",
    "@types/sha256": "^0.2.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-custom-rules": "^0.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^29.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nylas/nylas-nodejs.git"
  }
}
